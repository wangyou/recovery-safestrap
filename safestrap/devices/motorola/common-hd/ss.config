# Device specific
SYSTEM=mmcblk0p36
USERDATA=mmcblk0p38
CACHE=mmcblk0p35
BOOT=mmcblk0p31
SYSTEM_FSTYPE=ext4
USERDATA_FSTYPE=ext4
SS_FSTYPE=ext4
SS_PART=mmcblk0p38
SS_DIR=/ss/safestrap
HIJACK_BIN=init.qcom.modem_links.sh
HIJACK_LOC=etc
BOOTMODE_PROP=ro.bootmode
DEVICE_PROP=ro.product.device
CHARGER_MODE_SYSFS=/sys/class/power_supply/ac/online
CHECK_BOOTMODE=bp-tools
CHECK_POWERUP_REASON=/sbin/bbx cat /proc/bootinfo | /sbin/bbx grep POWERUPREASON | /sbin/bbx cut -c 19-
POWERUP_REASON_CHARGER=00000100
BACKLIGHT_BRIGHTNESS_PATH=/sys/class/backlight/lcd-backlight/brightness
BACKLIGHT_BRIGHTNESS_VALUE=200
TASKSET_CPUS=0-1
SS_USE_DATAMEDIA=1

